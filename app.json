{
  "expo": {
    "name": "iNoted",
    "slug": "iNoted",
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.iNoted",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "com.app.expo-router-inoted",
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-sqlite",
      "react-native-edge-to-edge",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f2f3f4",
          "image": "./assets/splash.png",
          "dark": {
            "backgroundColor": "#0f0f0f",
            "image": "./assets/splash_dark.png"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow iNoted to access your photos.",
          "cameraPermission": "Allow iNoted to access your camera."
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/icons/app.png",
            "./assets/icons/app_long.png",
            "./assets/icons/dark_app_long.png",
            "./assets/showcase/showcase_1.png",
            "./assets/showcase/showcase_2.png",
            "./assets/notebooks/notebook_1.png",
            "./assets/notebooks/notebook_2.png",
            "./assets/notebooks/notebook_3.png",
            "./assets/notebooks/notebook_select.png"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b74a4997-ad2d-41fd-99be-05777273a364"
      }
    },
    "owner": "nincloud"
  }
}
