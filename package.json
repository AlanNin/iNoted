{
  "name": "inoted",
  "version": "1.1.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start-go": "expo start --go",
    "start-dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean-eas-dev-build-android": "npx drizzle-kit generate && npx expo prebuild -p android --clean && eas build --profile development --platform android",
    "eas-dev-build-android": "eas build --profile development --platform android",
    "clean-eas-prod-build-android": "npx drizzle-kit generate && npx expo prebuild -p android --clean && eas build --platform android",
    "eas-prod-build-android": "eas build --platform android",
    "clean-local-dev-build-android": "npx drizzle-kit generate && npx expo prebuild -p android --clean && npx expo run:android",
    "clean-local-prod-build-android": "npx drizzle-kit generate && npx expo prebuild -p android --clean && npx expo run:android --variant release",
    "local-dev-build-android": "npx expo run:android",
    "local-prod-build-android": "npx expo run:android --variant release"
  },
  "dependencies": {
